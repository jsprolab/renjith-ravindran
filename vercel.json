{
  "functions": {
    "api/contacts/index.js": {
      "maxDuration": 10
    },
    "api/contacts/[id].js": {
      "maxDuration": 10
    },
    "api/email/send.js": {
      "maxDuration": 10
    },
    "api/auth/login.js": {
      "maxDuration": 10
    },
    "api/admin/contacts.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm ci --legacy-peer-deps && npm run build",
  "installCommand": "npm ci --legacy-peer-deps",
  "outputDirectory": "build"
}
