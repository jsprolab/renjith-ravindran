{
  "functions": {
    "api/contacts/index.js": {
      "maxDuration": 10
    },
    "api/contacts/[id].js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "buildCommand": "npm install --legacy-peer-deps && npm run build"
}
